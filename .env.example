# Telegram bot token from BotFather
BOT_TELEGRAM_TOKEN=123456:ABCDEF

# SQLAlchemy async DSN
BOT_DATABASE__DSN=mysql+asyncmy://bot:bot@localhost:3306/telegram_bot
BOT_DATABASE__POOL_SIZE=5
BOT_DATABASE__MAX_OVERFLOW=10

# Conversation model provider（openai / azure / zhipu / gemini / custom）
BOT_LLM__PROVIDER=openai
BOT_LLM__MODEL=gpt-4o-mini

# OpenAI-compatible creds（OpenAI / custom / zhipu / gemini 共用）
BOT_LLM__OPENAI__API_KEY=sk-xxxxxxxx
BOT_LLM__OPENAI__BASE_URL=https://api.openai.com/v1

# Azure OpenAI creds（provider=azure/azure_openai 时必填）
BOT_LLM__AZURE__API_KEY=
BOT_LLM__AZURE__BASE_URL=https://your-resource-name.openai.azure.com
BOT_LLM__AZURE__API_VERSION=2024-12-01-preview

# 智谱 Zhipu（OpenAI 兼容）
BOT_LLM__ZHIPU__API_KEY=
BOT_LLM__ZHIPU__BASE_URL=https://open.bigmodel.cn/api/paas/v4/chat/completions

# Google Gemini（OpenAI proxy）
BOT_LLM__GEMINI__API_KEY=
BOT_LLM__GEMINI__BASE_URL=

# 自建 OpenAI-Compatible 服务
BOT_LLM__CUSTOM__API_KEY=
BOT_LLM__CUSTOM__BASE_URL=

# Summary model overrides（可选，缺省复用主模型配置）
BOT_SUMMARY__PROVIDER=
BOT_SUMMARY__MODEL=

# Collaborator agent model overrides (optional, defaults to main model)
BOT_COLLABORATOR__PROVIDER=
BOT_COLLABORATOR__MODEL=

 # 可选，图片描述 agent 的 provider，默认沿用主模型 # 可选，图片描述 agent 的模型名称，默认沿用主模型
BOT_VISION__PROVIDER=             
BOT_VISION__MODEL=                 

# Rate limit per user per hour
BOT_REQUEST_LIMIT__MAX_REQUESTS=5
BOT_REQUEST_LIMIT__INTERVAL_SECONDS=10
BOT_REQUEST_LIMIT__WINDOW_RETENTION_HOURS=48

# External tools（SerpApi / Jina Reader / Judge0）
BOT_EXTERNAL_TOOLS__SERPAPI_API_KEY=
BOT_EXTERNAL_TOOLS__SERPAPI_ENGINE=google_light
BOT_EXTERNAL_TOOLS__JINA_READER_BASE_URL=https://r.jina.ai/
BOT_EXTERNAL_TOOLS__JINA_READER_API_TOKEN=
BOT_EXTERNAL_TOOLS__JUDGE0_API_URL=
BOT_EXTERNAL_TOOLS__JUDGE0_API_KEY=
BOT_EXTERNAL_TOOLS__JUDGE0_LANGUAGE_ID=71
BOT_EXTERNAL_TOOLS__REQUEST_TIMEOUT_SECONDS=10
BOT_EXTERNAL_TOOLS__MARKET_SNAPSHOT_SECRET_KEY=

# Misc
BOT_ENVIRONMENT=dev
BOT_TELEGRAM_PROXY=
BOT_ADMIN_TELEGRAM_ID=123456789
